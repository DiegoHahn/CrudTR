{
  "name": "crudtr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "angular": "^1.8.3",
    "cli": "^1.0.1",
    "install": "^0.13.0",
    "jest-preset-angular": "^13.1.6",
    "ng-mat-select-infinite-scroll": "^4.0.0",
    "ngx-mask": "^10.0.0",
    "rxjs": "~7.5.0",
    "ts-jest": "^29.1.3",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "typescript": "~4.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/app/components/clients/client/client.component.spec.ts",
      "<rootDir>/src/app/components/clients/create-client/create-client.component.spec.ts",
      "<rootDir>/src/app/components/clients/edit-client/edit-client.component.spec.ts",
      "<rootDir>/src/app/components/clients/list-clients/list-clients.component.spec.ts",
      "<rootDir>/src/app/components/clients/delete-client/delete-client.component.spec.ts",
      "<rootDir>/src/app/components/footer/footer.component.spec.ts",
      "<rootDir>/src/app/components/header/header.component.spec.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  }
}
